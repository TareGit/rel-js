{
    "queueTimeout": 300000,
    "queueItemsPerPage": 10,
    "maxQueueFetchTime": 5000,
    "defaultPrimaryColor": "#2f3136",
    "defaultPrefix": "?",
    "defaultLanguage": "en",
    "defaultNickname": "Umeko",
