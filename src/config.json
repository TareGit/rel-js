    "guildSettingsTableFormat": {
        "name": "guild_settings",
        "rows": [
            {
                "name": "id",
                "type": "TEXT",
                "option": "PRIMARY KEY"
            },
            {
                "name": "color",
                "type": "TEXT",
                "option": "DEFAULT '#2f3136'"
            },
            {
                "name": "prefix",
                "type": "TEXT",
                "option": "DEFAULT '?'"
            },
            {
                "name": "nickname",
                "type": "TEXT",
                "option": "DEFAULT 'Umeko'"
            },
            {
                "name": "language",
                "type": "TEXT",
                "option": "DEFAULT 'en'"
            },
            {
                "name": "welcome_message",
                "type": "TEXT",
                "option": "DEFAULT 'Welcome to the server {user}'"
            },
            {
                "name": "welcome_options",
                "type": "TEXT",
                "option": "DEFAULT ''"
            },
            {
                "name": "twitch_message",
                "type": "TEXT",
                "option": "DEFAULT '{username} just went live, check them out :{link} .'"
            },
            {
                "name": "twitch_options",
                "type": "TEXT",
                "option": "DEFAULT ''"
            },
            {
                "name": "leveling_message",
                "type": "TEXT",
                "option": "DEFAULT 'Congrats on reaching level {level} {user} !'"
            },
            {
                "name": "leveling_options",
                "type": "TEXT",
                "option": "DEFAULT ''"
            }
        ]
    },
    "userSettingsTableFormat": {
        "name": "user_settings",
        "rows": [
            {
                "name": "id",
                "type": "TEXT",
                "option": "PRIMARY KEY"
            },
            {
                "name": "color",
                "type": "TEXT",
                "option": "DEFAULT '#87ceeb'"
            },
            {
                "name": "card_bg_id",
                "type": "TEXT",
                "option": "DEFAULT ''"
            },
            {
                "name": "card_bg_url",
                "type": "TEXT",
                "option": "DEFAULT ''"
            },
            {
                "name": "card_opacity",
                "type": "REAL",
                "option": "DEFAULT 0.8"
            },
            {
                "name": "afk_message",
                "type": "TEXT",
                "option": "DEFAULT 'Im sleeping or something'"
            },
            {
                "name": "afk_options",
                "type": "TEXT",
                "option": "DEFAULT ''"
            }
        ]
    },
    "guildLevelingTableFormat": {
        "name": "guild_leveling_",
        "rows": [
            {
                "name": "userId",
                "type": "TEXT",
                "option": "PRIMARY KEY"
            },
            {
                "name": "level",
                "type": "INTEGER",
                "option": "DEFAULT 0"
            },
            {
                "name": "xp_current",
                "type": "INTEGER",
                "option": "DEFAULT 0"
            }
        ]
    },
    "guildCommandsTableFormat": {
        "name": "guild_commands_",
        "rows": [
            {
                "name": "commandId",
                "type": "TEXT",
                "option": "PRIMARY KEY"
            },
            {
                "name": "role_overiddes",
                "type": "TEXT",
                "option": "DEFAULT ''"
            },
            {
                "name": "channel_overiddes",
                "type": "TEXT",
                "option": "DEFAULT ''"
            }
        ]
    },
    "xpUpdateThreshold" : 100
