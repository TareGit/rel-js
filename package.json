{
  "name": "rel-js",
  "version": "1.5.0",
  "description": "A Discord Bot",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node --es-module-specifier-resolution=node ./dist/index.js",
    "debug": "node --es-module-specifier-resolution=node ./dist/index.js --no-cache --no-cluster --debug",
    "build": "npm run clean && npx tsc && npx tsc-alias",
    "watch": "npm run clean && npx conc \"npx tsc -w\" \"npx tsc-alias -w\"",
    "pretty": "npx prettier --write .",
    "clean": "node ./scripts/clean.js"
  },
  "keywords": [],
  "author": "Oyintare",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.6.0",
    "axios": "^0.24.0",
    "chokidar": "^3.5.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "discord-api-types": "^0.24.0",
    "discord-hybrid-sharding": "^1.4.3",
    "discord.js": "^13.2.0",
    "express": "^4.17.1",
    "ffmpeg-static": "^4.4.0",
    "heatsync": "^2.2.1",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "lavacord": "^1.1.9",
    "libsodium-wrappers": "^0.7.9",
    "node-os-utils": "^1.3.5",
    "opusscript": "^0.0.8",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-adblocker": "^2.13.5",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "spotify-url-info": "^3.1.2"
